{
  "rulesDirectory": ["tslint-plugin-prettier"],
  "extends": ["tslint:latest", "tslint-react", "tslint-config-prettier"],
  "rules": {
    "prettier": true,
    "no-implicit-dependencies": [true, "dev"],
    "no-submodule-imports": [true, "swiper", "react-id-swiper"],
    "jsx-no-lambda": false
  },
  "linterOptions": {
    "exclude": ["build", "src/serviceWorker.ts", "src/setupTests.ts"]
  }
}
