  "scripts": {
    "start": "BROWSER=none react-scripts start",
    "build": "sh -ac '. ./.env.${REACT_APP_ENV}; react-scripts build'",
    "build:staging": "REACT_APP_ENV=staging react-scripts build",
    "build:dev": "REACT_APP_ENV=dev react-scripts build",
    "build:all": "yarn build && yarn build-storybook",
    "test": "TZ=UTC REACT_APP_API_BASE_PATH=https://jinglebets.com react-scripts test",
    "eject": "react-scripts eject",
    "test:single": "TZ=UTC CI=true REACT_APP_API_BASE_PATH=https://jinglebets.com react-scripts test --watchAll=false --verbose --colors",
    "lint": "concurrently \"yarn run lint:es\" \"yarn run lint:scss\"",
    "lint:es": "eslint '*/**/*.{js,ts,tsx}'",
    "lint:scss": "stylelint \"src/**/*.scss\"",
    "format": "prettier --write './**/*.{ts,tsx,scss,md,json,js}'",
    "storybook": "start-storybook --ci -p 9009 -s .storybook/public",
    "build-storybook": "build-storybook -s .storybook/public -o build/storybook",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "deploy-dev": "yarn build && yarn build-storybook && sls deploy -v -s dev",
    "postinstall": "patch-package",
    "apollo": "node ./scripts/apollo/generatePossibleTypes.js"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version",
      "Edge 18"
    ]
  },
  "jest": {
    "transformIgnorePatterns": [
      "node_modules/(?!(swiper|react-id-swiper|dom7)/)"
    ]
  },
  "devDependencies": {
    "@storybook/addon-a11y": "^6.0.28",
    "@storybook/addon-actions": "^6.0.28",
    "@storybook/addon-backgrounds": "^6.0.28",
    "@storybook/addon-knobs": "^6.0.28",
