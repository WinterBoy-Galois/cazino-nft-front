{
  "name": "cazzino-test-project",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@apollo/react-hooks": "^3.1.3",
    "@reach/router": "^1.3.3",
    "@testing-library/jest-dom": "^5.8.0",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^10.3.2",
    "@types/jest": "^25.2.3",
    "@types/node": "^14.0.4",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "@types/react-transition-group": "^4.2.4",
    "@types/swiper": "^5.3.1",
    "apollo-cache-inmemory": "^1.6.6",
    "apollo-client": "^2.6.10",
    "apollo-link": "^1.2.14",
    "apollo-link-http": "^1.5.17",
    "apollo-link-ws": "^1.0.20",
    "apollo-utilities": "^1.3.4",
    "bootstrap-scss": "^4.5.0",
    "graphql": "^15.0.0",
    "graphql-tag": "^2.10.3",
    "i18next": "^19.4.4",
    "i18next-browser-languagedetector": "^4.2.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-i18next": "^11.4.0",
    "react-id-swiper": "^3.0.0",
    "react-scripts": "3.4.1",
    "react-transition-group": "^4.4.1",
    "subscriptions-transport-ws": "^0.9.16",
    "swiper": "^5.4.1",
    "typescript": "~3.8.3",
    "uuid": "^8.1.0"
  },
  "scripts": {
    "start": "BROWSER=none react-scripts start",
    "build": "react-scripts build",
    "build:all": "yarn build && yarn build-storybook",
    "test": "TZ=UTC react-scripts test",
    "eject": "react-scripts eject",
    "test:single": "TZ=UTC CI=true react-scripts test --watchAll=false",
    "lint": "concurrently \"yarn run lint:ts\" \"yarn run lint:scss\"",
    "lint:ts": "tslint --project .",
    "lint:scss": "stylelint \"src/**/*.scss\"",
    "format": "prettier --write './**/*.{ts,tsx,scss,md,json,js}'",
    "storybook": "start-storybook --ci -p 9009 -s .storybook/public",
    "build-storybook": "build-storybook -s .storybook/public -o build/storybook",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "deploy-dev": "yarn build && yarn build-storybook && sls deploy -v -s dev"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version",
      "Edge 18"
    ]
  },
  "jest": {
    "transformIgnorePatterns": [
      "node_modules/(?!(swiper|react-id-swiper|dom7)/)"
    ]
  },
  "devDependencies": {
    "@storybook/addon-actions": "^5.3.18",
    "@storybook/addon-knobs": "^5.3.18",
    "@storybook/addon-links": "^5.3.18",
    "@storybook/addon-viewport": "^5.3.18",
    "@storybook/addons": "^5.3.18",
    "@storybook/preset-create-react-app": "^2.1.2",
    "@storybook/react": "^5.3.18",
    "@testing-library/react-hooks": "^3.2.1",
    "@types/preval.macro": "^3.0.0",
    "@types/react-transition-group": "^4.2.4",
    "@types/uuid": "^8.0.0",
    "concurrently": "^5.2.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.4",
    "mock-apollo-client": "^0.4.0",
    "node-sass": "^4.14.1",
    "playwright": "^1.0.2",
    "prettier": "^2.0.5",
    "preval.macro": "^5.0.0",
    "react-test-renderer": "^16.13.1",
    "serverless-s3-sync": "^1.14.2",
    "source-map-explorer": "^2.4.2",
    "stylelint": "^13.5.0",
    "stylelint-config-prettier": "^8.0.1",
    "stylelint-config-recommended-scss": "^4.2.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-scss": "^3.17.2",
    "tslint": "^6.1.2",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.3.0",
    "tslint-react": "^5.0.0"
  }
}
