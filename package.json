  "scripts": {
    "start": "BROWSER=none react-scripts start",
    "build": "react-scripts build",
    "build:all": "yarn build && yarn build-storybook",
    "test": "TZ=UTC react-scripts test",
    "eject": "react-scripts eject",
    "test:single": "TZ=UTC CI=true react-scripts test --watchAll=false --verbose --colors",
    "lint": "concurrently \"yarn run lint:es\" \"yarn run lint:scss\"",
    "lint:es": "eslint '*/**/*.{js,ts,tsx}'",
    "lint:scss": "stylelint \"src/**/*.scss\"",
    "format": "prettier --write './**/*.{ts,tsx,scss,md,json,js}'",
    "storybook": "start-storybook --ci -p 9009 -s .storybook/public",
    "build-storybook": "build-storybook -s .storybook/public -o build/storybook",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "deploy-dev": "yarn build && yarn build-storybook && sls deploy -v -s dev"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version",
      "Edge 18"
    ]
  },
  "jest": {
    "transformIgnorePatterns": [
      "node_modules/(?!(swiper|react-id-swiper|dom7)/)"
    ]
  },
  "devDependencies": {
    "@storybook/addon-a11y": "^5.3.19",
    "@storybook/addon-actions": "^5.3.19",
    "@storybook/addon-backgrounds": "^5.3.19",
    "@storybook/addon-knobs": "^5.3.19",
    "@storybook/addon-links": "^5.3.19",
    "@storybook/addon-viewport": "^5.3.19",
    "@storybook/addons": "^5.3.19",
    "@storybook/preset-create-react-app": "^3.0.0",
    "@storybook/react": "^5.3.19",
    "@testing-library/react-hooks": "^3.2.1",
    "@types/jwt-decode": "^2.2.1",
    "@types/preval.macro": "^3.0.0",
    "@types/react-helmet": "^6.0.0",
    "@types/react-transition-group": "^4.4.0",
    "@types/uuid": "^8.0.0",
    "@types/yup": "^0.29.3",
    "concurrently": "^5.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.20.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.6",
    "mock-apollo-client": "^0.4.0",
    "node-sass": "^4.14.1",
    "playwright": "^1.0.2",
    "prettier": "^2.0.5",
    "preval.macro": "^5.0.0",
    "react-test-renderer": "^16.13.1",
    "serverless-s3-sync": "^1.14.3",
    "source-map-explorer": "^2.4.2",
    "stylelint": "^13.5.0",
    "stylelint-config-prettier": "^8.0.1",
    "stylelint-config-recommended-scss": "^4.2.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-scss": "^3.17.2"
  }
}
