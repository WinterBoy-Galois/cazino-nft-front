    "@types/uuid": "^8.3.0",
    "@types/yup": "^0.29.9",
    "concurrently": "^5.3.0",
    "cross-fetch": "^3.0.6",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.5",
    "husky": "^4.3.0",
    "lint-staged": "^10.5.1",
    "node-sass": "^4.14.1",
